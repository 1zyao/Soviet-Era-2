{
    "multipart": [
        {   "when": { "default": "true" },
            "apply": { "model": "soviet:block/innerdeco/pipes/ventpipe" }
        },
     	{   "when": { "down": "true" },
            "apply": { "model": "soviet:block/innerdeco/pipes/ventpipe_down", "uvlock": true }
        },
     	{   "when": { "up": "true" },
            "apply": { "model": "soviet:block/innerdeco/pipes/ventpipe_up", "uvlock": true }
        },
        {   "when": { "north": "true" },
            "apply": { "model": "soviet:block/innerdeco/pipes/ventpipe_north", "uvlock": true }
        },
        {   "when": { "east": "true" },
            "apply": { "model": "soviet:block/innerdeco/pipes/ventpipe_east", "uvlock": true }
        },
        {   "when": { "south": "true" },
            "apply": { "model": "soviet:block/innerdeco/pipes/ventpipe_south","uvlock": true }
        },
        {   "when": { "west": "true" },
            "apply": { "model": "soviet:block/innerdeco/pipes/ventpipe_west", "uvlock": true }
        }
    ]
}
