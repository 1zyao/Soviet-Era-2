        {   "when": { "facing": "north", "left": "true" },
            "apply": { "model": "soviet:block/outerdeco/electro_fence_left" }
        },
        {   "when": { "facing": "north", "right": "true" },
            "apply": { "model": "soviet:block/outerdeco/electro_fence_right" }
